<%- include("../header") %>
  <!--Container Main start-->
  <div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head bet-monitoring-heading"> 
          <h3>Alert Bet</h3>
          <form action="" class="betmonitor-heading-from">
            <div class="input-group">
              <label class="up-down-form">Select User</label>
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-magnifying-glass"></i></span>
              </div>
              <input type="text" id="searchUser" list="text_editors" class="form-control searchUser" placeholder="search user" aria-label="Username" aria-describedby="basic-addon1" autocomplete="off">
              <div class="wrapper">
                <ul id="search" class="users">
              
                </ul>
                <ul id="button" class="button">
                  
                </ul>
              </div>
              
            </div>
            <div class="input-group">
              <label class="up-down-form">From Date</label>
              <input type="date" id="fromDate">
            </div>
            <div class="input-group">
              <label class="up-down-form">To Date</label>
              <input type="date" id="toDate">
            </div>
            <div class="input-group">
              <label>Select Result</label>
              <select name="result" id="result">
                <option value="All" selected>All</option>
                <option value="ALERT">Alert</option>
                <option value="CANCEL">Voided</option>
                <option value="ACCEPT">Valid</option>
              </select>
            </div>
            <div class="input-group">
              <label class="up-down-form">Select Sports</label>
              <select name="Sport" id="Sport">
                <option value="All" selected>All</option>
                <option value="Cricket">Cricket</option>
                <option value="Football">FOOTBALL</option>
                <option value="Tennis">TENNIS</option>
                <option value="BASKETBALL">BASKETBALL</option>
                <option value="BASEBALL">BASEBALL</option>
              </select>
            </div>
            <div class="input-group">
              <label class="up-down-form">Select Market</label>
              <select name="market" id="market">
                <option value="All" selected>All</option>
                <option value="Match Odds">Match Odds</option>
                <option value="Bookmaker 0%Comm">BookMaker</option>
                <option value="Fancy">Fancy</option>
                <!-- <option value="">GoalMarket</option> -->
              </select>
            </div>
            
            <div class="input-group">
              <button type="button" class="refresh btn-group">Refresh</button>
            </div>
          </form>
         
      </div>
  </section>

  <section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading">
                <h3>Alert Bet</h3>
                <div class="table-heading-btn">
                  
                </div>
            </div>
            <div class="data-table">
              <%- include("./table") %>
            </div>
        </div>
    </div>
</section>
<div class="pageId" data-pageid="1"></div>
</div>

<%if(bets.length != 0){%>
  <div id="load-more">
    <button class="load-more">Load More</button>
  </div>
<%}else{%>
  <div id="load-more" style="display: none;">
    <button class="load-more">Load More</button>
  </div>
<%}%>

<div class="modal fade" id="myModaladduser" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title">Add HouseFunds</h4>
      </div>
      <div class="modal-body">
        <form class="form-data">
          <div class="row">
            <div class="col">
              <label for="">Amounts</label>
              <input type="text" class="form-control ml0" placeholder="1000000" name="email">
            </div>
            <div class="col">

            </div>
          </div>
          <button type="button" class="btn-submit mt-3">AddFunds &nbsp; <img src="../images/sub.png" alt=""></button>
        </form>
      </div>
    </div>
    
  </div>
</div>

<div class="modal fade " id="myModal2" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">Ã—</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal"></button> -->
        <h4 class="modal-title">Voide Bets</h4>
      </div>
      <div class="modal-body">
        <form class="form-data timely-voideBet">
          <div class="row">
              <div class="col">
                <label for="">password</label>
