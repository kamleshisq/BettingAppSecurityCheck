<div id="Cricket" class="tabcontent">
  <div class="row">
    <%for(let i = 0; i < openBet.length; i++){%>
      <%if(openBet[i].bettype == "Cricket"){%>
        <section>
          <div class="container-fluid">
            <div class="new-head">
              <h3 class="head-hr"><%=  openBet[i].details[0].match %></h3>
              <a class="btn-filter" >View Details &nbsp;<i class="fa-solid fa-chevron-right"></i></a>
            </div>
            <%for(let j = 0 ; j < openBet[i].details.length; j++){%>
              <div class="video-section-1">
                <div class="video-play">
                  <div class="hed-data">
                    <h3 class="live-head"> <%=openBet[i].details[j].marketName%> </h3>
                  </div>
                  <div class="full-adta">
                    <table>
                      <thead>
                        <tr>
                          <th>OUTCOMES/RUNNER</th>
                          <th>EXPOSURE</th>
                          <th>TOTAL BETS</th>
                        </tr>
                      </thead>
                      <tbody>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            <%}%>
            </div> 
          </section>
      <%}%>
    <%}%>
    <%if(!openBet.some(item => item.bettype == "Cricket")){%>
      No data to display
    <%}%>
  </div>
  <!-- <button class="btn btn-view-all">View All</button> -->
</div>