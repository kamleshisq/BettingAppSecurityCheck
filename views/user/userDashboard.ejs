<!DOCTYPE html>
<html lang="en">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
<head>
    <style>
        ul {
          list-style-type: none;
          margin: 0;
          padding: 0;
          overflow: hidden;
          background-color: #ffffff;
        }
        
        li {
          float: left;
        }
        
        li a {
          display: block;
          color: rgb(0, 0, 0);
          text-align: center;
          padding: 14px 16px;
          text-decoration: none;
        }
        
        li a:hover {
          background-color: #ffffff;
        }
        </style>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

    <title>Home</title>
</head>
<body>
    <ul>
        <%if(user){%>
            <li><button type="button"  class="logOut"><a>logOut</a></button></li>
            <li><button class="edit"><a href="/edit">Edit</a></button></li>
            <li><button class="account"><a href="/myAccountStatment">My Account Statement</a></button></li>
            <li><button class="account"><a href="/changeUserPassword">change password</a></button></li>
        <%}else{%>
            <li><button class="account"><a href="/userlogin">Login</a></button></li>
        <%}%>
            <li><button class="account"><a href="/ALLGAMEFORTESTING">Casino</a></button></li>
            <li><button class="account"><a href="/SPORT">Sport Radar</a></button></li>
            <li><button class="account"><a href="/exchange_sports/inplay">Exchange</a></button></li>
      </ul>

      <div class="box_section">
      <%for(let i = 0; i < data.length; i++){%>
        <%if(data[i].status){%>
            <%if(data[i].video){%>
              <div class="box">
                <a id="foo" class="foo" onclick="buttonclick('<%=data[i]._id%>');"  >
                    <video width="400" controls >
                        <source src="img/<%=data[i].position%>.mp4" type="video/mp4">
                    </video>
                </a>
              </div>
            <%}else{%>
              <div class="box">
            
                <a id="foo" class="foo"  onclick="buttonclick('<%=data[i]._id%>');">
                    <img src="img/<%=data[i].position%>.png" alt="img"  width="500" height="600"><br>
                </a>
                </div>
            <%}%>
            <%}%>
            <%}%>
          </div>
<script src="userJs/logOut.js"></script>
<script >
  let id
  let on 
  const socket = io();
  function buttonclick(data){
          id = data
          on = true
      }
      socket.on('connect', () => {
      console.log("websocket Connected")
      function emitSocket(){
          if(on){
              socket.emit('UpdateBYID', id)
              on = false
              window.location.href = 'https://www.isquaretechnologies.com/'
          }
          setTimeout(()=>{
              emitSocket()
          }, 300)
      }
      emitSocket()
  })
      
  
  </script>
</body>
</html>