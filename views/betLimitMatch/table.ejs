<section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading">
                <h3>Bet Limits</h3>
            </div>
            <div class="data-table">
              <div class="rowId" data-rowid="1"></div>
  <table class="bet-list-table">
      <%if(seriesMatch.length > 0){%>
        <thead>
            <tr>
                <th>Market</th>
                <th>Min Stake</th>
                <th>Max Stake </th>
                <th>Max Profit</th>
                <th>Max Odd</th>
                <th>Delay</th>
                <th>Actions </th>
                </tr>
        </thead>
        <tbody class="new-body">
            <%for (var key in seriesMatch[0].marketList) {%>
                <%if (seriesMatch[0].marketList.hasOwnProperty(key) && seriesMatch[0].marketList[key] != null) {%>
                    <%if(betLimitMatchWise){%>
                       <% 
                        let marketData = betLimitMatchWise.marketDetails.find(item => item.title == key)
                        if(marketData){
                            
                                let min_stake = marketData.value.find(item => item.title === 'min_stake')
                                let max_stake = marketData.value.find(item => item.title === 'max_stake')
                                let max_profit = marketData.value.find(item => item.title === 'max_profit')
                                let max_odd = marketData.value.find(item => item.title === 'max_odd')
                            %>
                            <tr>
                                <td id="<%=series%>/<%=key%>" class="btn-filter"><%=key%></td>
                                <td><input type="text" class="form-datas" value="<%=min_stake.value%>" readonly=""></td>
                                <td><input type="text" class="form-datas" value="<%=max_stake.value%>" readonly=""></td>
                                <td><input type="text" class="form-datas" value="<%=max_profit.value%>" readonly=""></td>
                                <td><input type="text" class="form-datas" value="<%=max_odd.value%>" readonly=""></td>
                                <td><input type="text" class="form-datas" value="00" readonly=""></td>
                                <td>
                                  <div class="btn-group">
                                    <button type="button" data-bs-toggle="modal" data-bs-target="#myModal2"class="btn update updateBetLimitMATCH">Update</button>
                                  </div>
                                </td>
                              </tr>
                       
                       <%}else{%>
                        <tr>
                            <td id="<%=series%>/<%=key%>" class="btn-filter"><%=key%></td>
                            <td><input type="text" class="form-datas" value="100" readonly=""></td>
                            <td><input type="text" class="form-datas" value="1000" readonly=""></td>
                            <td><input type="text" class="form-datas" value="500" readonly=""></td>
                            <td><input type="text" class="form-datas" value="100" readonly=""></td>
                            <td><input type="text" class="form-datas" value="10" readonly=""></td>
                            <td>
                              <div class="btn-group">
                                <button type="button" data-bs-toggle="modal" data-bs-target="#myModal2"class="btn update updateBetLimitMATCH">Update</button>
                              </div>
                            </td>
                          </tr>
                        <%}%>
                    <%}else{%>
                        <tr>
                            <td id="<%=series%>/<%=key%>" class="btn-filter"><%=key%></td>
                            <td><input type="text" class="form-datas" value="100" readonly=""></td>
                            <td><input type="text" class="form-datas" value="1000" readonly=""></td>
                            <td><input type="text" class="form-datas" value="500" readonly=""></td>
                            <td><input type="text" class="form-datas" value="100" readonly=""></td>
                            <td><input type="text" class="form-datas" value="10" readonly=""></td>
                            <td>
                              <div class="btn-group">
                                <button type="button" data-bs-toggle="modal" data-bs-target="#myModal2"class="btn update updateBetLimitMATCH">Update</button>
                              </div>
                            </td>
                          </tr>
                    <%}%>
                <%}%>
            <%}%>
        </tbody>
    <%}else{%>
        <tr class="empty_table"><td>No <%=gameName%> Series! </td></tr>
    <%}%>
  </table>
            </div>
        </div>
    </div>
  </section>