<section>
  <div class="container-fluid">
      <div class="table-data-section">
          
          <div class="data-table">
            <div class="rowId" data-rowid="1"></div>
<table class="bet-list-table">
    <%if(seriesMatch.length > 0){%>
      <thead>
          <tr>
              <th>Market</th>
              <th>Min Stake</th>
              <th>Max Stake </th>
              <th>Max Profit</th>
              <th>Max Odd</th>
              <th>Delay</th>
              <th>Actions </th>
              </tr>
      </thead>
      <tbody class="new-body">
        <%for (var key in seriesMatch[0].marketList) {%>
          <%if (seriesMatch[0].marketList.hasOwnProperty(key) && seriesMatch[0].marketList[key] != null ) {%>
            <%if(!Array.isArray(seriesMatch[0].marketList[key])){%>
              <%
              let data = betLimit.find(item => item.type == seriesMatch[0].marketList[key].marketId)
              if(data){
              %>
              <tr>
                <td id="<%=seriesMatch[0].marketList[key].marketId%>" class="btn-filter"><%=key%></td>
                <td><input type="text" class="form-datas" name="min_stake" value="<%=data.min_stake%>" ></td>
                <td><input type="text" class="form-datas" name="max_stake" value="<%=data.max_stake%>" ></td>
                <td><input type="text" class="form-datas" name="max_profit" value="<%=data.max_profit%>" ></td>
                <td><input type="text" class="form-datas" name="max_odd" value="<%=data.max_odd%>" ></td>
                <td><input type="text" class="form-datas" name="delay" value="<%=data.delay%>" ></td>
                <td>
                  <div class="btn-group">
                    <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                  </div>
                </td>
              </tr>
              <%}else{%>
              <tr>
                  <td id="<%=seriesMatch[0].marketList[key].marketId%>" class="btn-filter"><%=key%></td>
                  <td><input type="text" class="form-datas" name="min_stake" value="0" ></td>
                  <td><input type="text" class="form-datas" name="max_stake" value="0" ></td>
                  <td><input type="text" class="form-datas" name="max_profit" value="0" ></td>
                  <td><input type="text" class="form-datas" name="max_odd" value="0" ></td>
                  <td><input type="text" class="form-datas" name="delay" value="0" ></td>
                  <td>
                    <div class="btn-group">
                      <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                    </div>
                  </td>
                </tr>
                <%}%>
            <%}%>
          <%}%>
        <%}%>

        <%if(bookMaker){
          for(let i = 0; i < bookMaker.length; i++){
          let data = betLimit.find(item => item.type == bookMaker[i].marketId)
          if(data){%>
            <tr>
              <td id="<%=bookMaker[i].marketId%>" class="btn-filter"><%=bookMaker[i].title%></td>
              <td><input type="text" class="form-datas" name="min_stake" value="<%=data.min_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="<%=data.max_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="<%=data.max_profit%>" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="<%=data.max_odd%>" ></td>
              <td><input type="text" class="form-datas" name="delay" value="<%=data.delay%>" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
            <%}else{%>
            <tr> 
              <td id="<%=bookMaker[i].marketId%>" class="btn-filter"><%=bookMaker[i].title%></td>
              <td><input type="text" class="form-datas" name="min_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="0" ></td>
              <td><input type="text" class="form-datas" name="delay" value="0" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
        <%}}}%>


        <%if(Session){
          let data = betLimit.find(item => item.type == `${seriesMatch[0].eventData.eventId}_session`)
          if(data){%>

            <tr>
              <td id="<%=seriesMatch[0].eventData.eventId%>_session" class="btn-filter"><strong>sessions</strong></td>
              <td><input type="text" class="form-datas" name="min_stake" value="<%=data.min_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="<%=data.max_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="<%=data.max_profit%>" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="<%=data.max_odd%>" ></td>
              <td><input type="text" class="form-datas" name="delay" value="<%=data.delay%>" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
            <%}else{%>
            <tr> 
              <td id="<%=seriesMatch[0].eventData.eventId%>_session" class="btn-filter"><strong>sessions</strong></td>
              <td><input type="text" class="form-datas" name="min_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="0" ></td>
              <td><input type="text" class="form-datas" name="delay" value="0" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
            <%}
            for(let i = 0; i < Session.length; i++){
              let data = betLimit.find(item => item.type == Session[i].marketId)
            if(data){
            %>  
            <tr>
              <td id="<%=Session[i].marketId%>" class="btn-filter"><%=Session[i].title%></td>
              <td><input type="text" class="form-datas" name="min_stake" value="<%=data.min_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="<%=data.max_stake%>" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="<%=data.max_profit%>" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="<%=data.max_odd%>" ></td>
              <td><input type="text" class="form-datas" name="delay" value="<%=data.delay%>" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
            <%}else{%>
            <tr> 
              <td id="<%=Session[i].marketId%>" class="btn-filter"><%=Session[i].title%></td>
              <td><input type="text" class="form-datas" name="min_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_stake" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_profit" value="0" ></td>
              <td><input type="text" class="form-datas" name="max_odd" value="0" ></td>
              <td><input type="text" class="form-datas" name="delay" value="0" ></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn update updateBetLimitMATCH">Update</button>
                </div>
              </td>
            </tr>
              <%}}%>
        <%}%>
      </tbody>
  <%}else{%>
      <tr class="empty_table"><td>No <%=gameName%> Series! </td></tr>
  <%}%>
</table>
          </div>
      </div>
  </div>
</section>