<div class="modal fade" id="myModaladduser" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header" style="text-align: center;">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title" style="color: #fff;">Add Notification</h4>
      </div>
      <div class="modal-body">
        <form class="form-data addNotification" id="addNotification">
          <div class="row">
            <div class="col">
              <label for="">Title</label>
              <input type="text" class="form-control ml0" placeholder="" name="title">
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">Message</label>
              <input type="text" class="form-control ml0" placeholder="" name="message">
            </div>
          </div>
          <div class="row mt-2">
            <div class="col">
              <input type="checkbox" id="vehicle1" name="sports" value="sports">
              <label for="vehicle1"> Sports</label>
              
              
            </div>
            <div class="col">
              <input type="checkbox" id="vehicle2" name="Casino" value="Casino"> 
              <label for="vehicle2"> Casino</label>
              
            </div>
            <div class="col">
              
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">Start Date & Time</label>
              <input type="date" class="form-control ml0"  name="Sdate">
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">End Date & Time</label>
              <input type="date" class="form-control ml0"  name="Edate">
            </div>
          </div>
          
            <button type="submit" class="btn-green mt-3 btn-ju" >Save</button>
        </form>
      </div>
    </div>
    
  </div>
