<div class="modal fade" id="myModaladduser" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header" style="text-align: center;">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title" style="color: #fff;">Add Notification</h4>
      </div>
      <div class="modal-body">
        <form class="form-data addNotification" id="addNotification">
          <div class="row">
            <div class="col">
              <label for="">Title</label>
              <input type="text" class="form-control ml0" placeholder="Title" name="title" required>
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">Message</label>
              <input type="text" class="form-control ml0" placeholder="Message" name="message" required>
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">Start Date</label>
              <input type="date" class="form-control ml0"  name="Sdate" required>
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="">End Date</label>
              <input type="date" class="form-control ml0"  name="Edate" required>
            </div>
          </div>
            <button type="submit" class="btn-submit btn-green mt-3 btn-ju" >Save</button>
        </form>
      </div>
    </div>    
  </div>
</div>


<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
