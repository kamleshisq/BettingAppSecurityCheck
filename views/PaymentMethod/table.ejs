<table>
    <thead>
        <tr>
          <th>Account Holder Name</th>
          <th>Account Number</th>
          <th>Ifsc Code</th>
          <th>Bank Name</th>
          <th>Branch Name </th>
          <th>Display Name</th>
          <th>Payment Method</th>
          <th>Status</th>
          <th>Actions</th>                      
        </tr>
    </thead>
    <tbody class="new-body">
      <% for(let i = 0; i < paymentmethod.length; i++){%>
        <tr class="lay" data-trid="paymentmethod[i]._id">
        <td><%=paymentmethod[i].accountholdername%></td>
        <td class="text-nowrap" ><%=paymentmethod[i].accountnumber%></td>
        <td class="text-nowrap" ><%=paymentmethod[i].ifsccode%></td>
        <td class="text-nowrap"><%=paymentmethod[i].bankname%></td>
        <td class="text-nowrap" ><%=paymentmethod[i].branchname%></td>
        <td class="text-nowrap" ><%=paymentmethod[i].displayname%></td>
        <%if(paymentmethod[i].status){%>
          <td><span class="on-off">OFF<label class="switch on">
            <input type="checkbox" id="check" name="check" checked="checked">
            <span class="slider round"></span>
            </label>&nbsp; ON</span>
          </td>
        <%}else{%>
          <td>
            <span class="on-off">OFF<label class="switch">
              <input type="checkbox" id="check" name="check" checked="checked">
              <span class="slider round"></span>
              </label>&nbsp; ON</span>
          </td>
        <%}%>
        <td>
          <div class="btn-group">
            <button class="btn accept" data-docid="<%=paymentmethod[i]._id%>">Delete</button>
          </div>
        </td>
    </tr>
    <%}%>
    </tbody>
  </table>