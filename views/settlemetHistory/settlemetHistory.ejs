  <section>
    <div class="container-fluid">
      <div class="table-data-section">
        <div class="table-heading">
          <h3>Settelments History</h3>
          <div class="table-heading-btn">
            <form action="" class="settlement-heading-from">
              <%
                let maxDate = new Date().toJSON().slice(0,10)
              %>
              
              <div class="input-group">
                <label class="up-down-form">From Date</label>
                <input type = "date" id="Fdate" max="<%=maxDate%>">
              </div> 
              <div class="input-group">
                <label class="up-down-form">To Date</label>
                <input type = "date" id="Tdate" max="<%=maxDate%>">
              </div> 
              </form>
          </div>
        </div>
        <div class="data-table">
          <table>
            <thead>
                <tr>
                  <th>S.No</th>
                  <th>Settelment Date</th>
                  <th>Event Name</th>
                  <th>Market Name</th>
                  <th>Results</th>
                </tr>
            </thead>
            <tbody class="new-body">
              <%for(let i = 0; i < History.length; i++){%>
                <tr>
                  <%
                    var date = History[i].date
                  %>
                  <td><%= i+1%></td>
                  <td class="date-time"><%=date.getDate() + '-' +(date.getMonth() + 1) + '-' + date.getFullYear() + " "+ date.getHours() + ':' + date.getMinutes() +':' + date.getSeconds()%></td>
                  <td class="text-nowrap"><%= History[i].eventName%></td>
                  <td><%= History[i].marketName%></td>
                  <td><%= History[i].result%></td>
                </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      </div>
        </section>
      </div>
      <%if(History.length >= 10){ %>
      <div>
        <button class="load-more">Load More</button>
      </div>
      <%}%>
<!-- end gallary -->
