<%- include("../header") %>
  <!--Container Main start-->
  <div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head"> 
          <h3>User History</h3>
          <form action="">
            <div class="input-group">
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-magnifying-glass"></i></span>
              </div>
              <input type="text" id="searchUser" onkeyup="myFunction()" list=text_editors class="form-control searchUser" placeholder="search user" aria-label="Username" aria-describedby="basic-addon1" autocomplete="off">
              <div class="wrapper">
                <ul id="search" class="users"> </ul>
                <ul id="button" class="button"> </ul>
              </div>
            </div>
          </form>
      </div>
    <%- include("../loginUserAdminsideDetails") %>
  </section>

  <section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading">
                <div class="table-heading-btn">
                  <form action="" class="settlement-heading-from">
                    <div class="input-group">
                      <label class="up-down-form">From Date</label>
                      <input type="date" id="fromDate">
                    </div> 
                    <div class="input-group">
                      <label class="up-down-form">To Date</label>
                      <input type="date" id="toDate">
                    </div> 
                    </form>
                    <!-- <button class="btn-filter filter"><i class="fa-solid fa-filter"></i> Filter</button> -->
                </div>
            </div>
            <div class="data-table">
              <%- include("./table")%>
            </div>
        </div>
    </div>
</section>
  <div class="pageId" data-pageid="1"></div>
  </div>
  <div id="load-more" >
    <%if(Logs.length >= 10){ %>
      <button class="load-more">Load More</button>
      <%}%>
    </div>
  <script>
    function myFunction() {
      var input, filter, ul, li, a, i, txtValue;
      input = document.getElementById("searchUser");
      filter = input.value.toUpperCase();
      ul = document.getElementById("search");
      li = ul.getElementsByTagName("li");
      for (i = 0; i < li.length; i++) {
          txtValue = li.textContent || li.innerText;
      }
  }
  </script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
