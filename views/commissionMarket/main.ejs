                    var options = { 
                      year: 'numeric',
                      month: 'long',
                      day: 'numeric',
                      hour: 'numeric',
                      minute: 'numeric',
                      hour12: true
                  };
                  var formattedTime = date.toLocaleString('en-US', options);
                    %>
                    <li id="<%=cricket[i].evntID%>" class="matchName"><%=cricket[i].evntNm%>  (<%=formattedTime%>)</li>
                  <%}%>
                </ul>
