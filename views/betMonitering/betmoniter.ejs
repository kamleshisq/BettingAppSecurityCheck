<%- include("../header") %>
  <!--Container Main start-->
  <div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head bet-monitoring-heading"> 
        <h3>Bets Monitoring</h3>
        <form action="" class="betmonitor-heading-from">
          <div class="input-group">
            <label class="up-down-form">Select User</label>
            <div class="input-group-prepend">
              <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-magnifying-glass"></i></span>
            </div>
            <input type="text" id="searchUser"  list=text_editors class="form-control searchUser" placeholder="search user" aria-label="Username" aria-describedby="basic-addon1" autocomplete="off">
            <div class="wrapper">
              <ul id="search" class="users">
            
              </ul>
              <ul id="button" class="button">
                
              </ul>
            </div>
            
          </div>
          <div class="input-group">
            <label class="up-down-form">From Date</label>
            <input type = "date" id="fromDate">
          </div>
          <div class="input-group">
            <label class="up-down-form">To Date</label>
            <input type = "date" id="toDate">
          </div>
          <div class="input-group">
            <label>Select Sport</label>
            <select name="Sport" id="Sport">
              <option value="All" selected>All</option>
              <option value="4">CRICKET</option>
              <option value="1">FOOTBALL</option>
              <option value="2">TENNIS</option>
              <option value="10">BASKETBALL</option>
              <option value="20">BASEBALL</option>
            </select>
          </div>
          <div class="input-group">
            <label>Select Event</label>
            <select name="Event" id="Event">
              <option value="All">Select Event</option>
              <%for(let i = 0;i<events.length;i++){%>
                <option value="<%=events[i].eventId%>"><%=events[i]._id%></option>
              <%}%>
            </select>
          </div>
          <div class="input-group">
            <label>Select Market</label>
            <select name="market" id="market">
              <option value="All">All</option>
              <option value="Match Odds">Match Odds</option>
              <option value="Bookmaker 0%Comm">BookMaker</option>
              <option value="Fancy">Fancy</option>
            </select>
          </div>
          <%if(currentUser.role.role_level == 1){%>
            <div class="input-group">
              <label>Select Whitelabels</label>
              <select name="whiteLabel" id="whiteLabel">
                <option value="All" selected>All</option>
                <%for(let i = 0;i<whiteLabels.length;i++){%>
                  <option value="<%=whiteLabels[i].whiteLabelName%>"><%=whiteLabels[i].whiteLabelName%></option>
                <%}%>
              </select>
            </div>
          <%}%>
          
          
          <div class="input-group">
            <label>Select Result</label>
            <select name="result" id="result">
              <option value="All">All</option>
              <option value="WON">Won</option>
              <option value="LOSS">Lost</option>
              <option value="CANCEL">Void</option>
            </select>
          </div>
          <div class="input-group">
            <label class="up-down-form">Stake</label>
            <input type = "number" id="stake">
          </div>
          <div class="input-group">
            <label class="up-down-form">IP</label>
            <input type = "text" id="IP">
          </div>
          <div class="input-group">
            <button type="button" class="refresh btn-group">Refresh</button>
          </div>
        </form>
      </div>
  </section>

  <section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading betmonitor-heading">
                <h3>Bet Monitoring</h3>
                <div class="rowId" data-rowid="1"></div>
                <div class="table-heading-btn">
                  
                </div>
            </div>
            <div class="data-table">
              <%- include("./table") %>
            </div>
        </div>
    </div>
</section>
<div class="pageId" data-pageid="1"></div>
</div>
<%if(bets.length !== 0){%>
  <div id="load-more">
    <button class="load-more">Load More</button>
  </div>
<%}%>
<!-- seling section -->

<!--  -->

<!--  -->
<div class="modal fade" id="myModaladduser" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title">Add HouseFunds</h4>
      </div>
      <div class="modal-body">
        <form class="form-data">
          <div class="row">
            <div class="col">
              <label for="">Amounts</label>
              <input type="text" class="form-control ml0" placeholder="1000000" name="email">
            </div>
            <div class="col">

            </div>
          </div>
          <button type="button" class="btn-submit mt-3">AddFunds &nbsp; <img src="../images/sub.png" alt=""></button>
        </form>
      </div>
    </div>
    
  </div>
</div>

    

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/apexcharts@3.40.0/dist/apexcharts.min.js"></script>
<script type="text/javascript" src="../js/bundle.js"></script>


</body>
</html>