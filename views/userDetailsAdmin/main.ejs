                                    <td><%=bets[i].oddValue%></td>
                                    <td><%=bets[i].match%></td>
                                    <td><%=bets[i].selectionName%></td>
                                  <%}else{%>
                                      <td>-</td>
                                      <td>-</td>
                                      <td>-</td>
                                      <td>-</td>
                                  <%}%>
                                  <td><%=bets[i].Stake%></td>
                                  <td><%=bets[i].transactionId%></td>
                                  <td><%=bets[i].status%></td>
                                  <td><%=bets[i].returns%></td>
                              </tr>
                              <%}%>
                                
                               
                              </tbody>
                            </table>
