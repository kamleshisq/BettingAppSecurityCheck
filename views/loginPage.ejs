<!DOCTYPE html>
<html>
<head>
  <script>
    document.documentElement.className = 'js';
  </script>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" type="text/css" href="http://dev.ollscores.com/css/style.css" />
<link rel="stylesheet" type="text/css" href="http://dev.ollscores.com/css/responsive.css" />
<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script type="text/javascript" src="js/bundle.js"></script>
</head>
<body>
  <%let JsStatus=false%>
  <script>
    // If JavaScript is enabled, render the full page
    if (document.documentElement.classList.contains('js')) {
      JsStatus = true
        // document.write('<h1>JavaScript is enabled!</h1>');
        // Place your regular HTML content here
    } else {
        // If JavaScript is not enabled, render a blank page
        document.write('<h1>JavaScript is not enabled!</h1>');
        JsStatus = false
        // Optionally, redirect to a blank page
        // window.location.href = 'blank.html';
    }
</script>
<%if(JsStatus){%>
  <div class="loginForm-section">
    <div class="loginForm-wraper">
      <span class="adminSide-logo" >
        <img  src="../../assets/img/Royal777.png" alt="logo">
      </span>
      <form class= "login-form loginFormAdmin">
        <h2>Sign In</h2>
        <!-- <div class="imgcontainer">
          <img src="img_avatar2.png" alt="Avatar" class="avatar">
        </div> -->
        <div class="container">
          <label for="uname"><b>Username</b></label>
          <input type="text" placeholder="Enter Username" name="uname" required id=uname autofocus>
      
          <label for="psw"><b>Password</b></label>
          <input type="password" placeholder="Enter Password" name="psw" required id=password>
               
          <button type="submit">Sign In</button>
        </div>
      </form>
    </div>
</div>
<%}%>


<!--================================== JAVASCRIPT ==================================-->



</body>
</html>
