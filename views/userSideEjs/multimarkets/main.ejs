<script>
  // Function to handle the click event for the "tbl-bg-blu-spn" spans
  function handleSpanClick(event) {
    const span = event.target;
    if (!span.classList.contains('tbl-bg-blu-spn')) return;

    event.preventDefault();
    const targetId = span.getAttribute('href');
    const targetCollapseDiv = document.querySelector(targetId);

    // Get the closest "table-data-2" div
    const tableDataDiv = span.closest('.table-data-2');

    // Hide all other "my-exc-inn-colaps-txt-dv collapse" divs within the same "table-data-2" div
    const collapseDivs = tableDataDiv.querySelectorAll('.my-exc-inn-colaps-txt-dv.collapse');
    collapseDivs.forEach(div => {
      if (div !== targetCollapseDiv) {
        div.classList.remove('show');
      }
    });

    targetCollapseDiv.classList.toggle('show');
  }

  // Add a single click event listener to the document to handle all clicks
  document.addEventListener('click', handleSpanClick);
</script>
