    // Generate a random CAPTCHA code
    function generateCaptcha() {
      var captchaLength = 6; // Length of the captcha
      var charset = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"; // Characters used for captcha
      var captcha = "";
      for (var i = 0; i < captchaLength; i++) {
        var randomIndex = Math.floor(Math.random() * charset.length);
        captcha += charset.charAt(randomIndex);
      }
      return captcha;
    }

    // Set the CAPTCHA code to the span with ID "captchaSpan"
    var captchaCode = generateCaptcha();
    $("#captchaSpan").text(captchaCode);
  });
