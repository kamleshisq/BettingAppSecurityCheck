<%if(LiveCricket.length > 0 && upcomintCricket.length > 0){%>
  <table class="table table-hover exchange-pg-mytbl">
  <thead class="exchange-pg-mytbl-hed">
    <tr class="exchange-pg-mytbl-hed-tr">
      <th scope="col">Cricket</th>
      <th scope="col">1</th>
      <th scope="col">X</th>
      <th scope="col">2</th>
    </tr>
  </thead>
  <tbody class="exchange-pg-mytbl-body">
      <% let cricketNum = 1 %>
      <%for(let i = 0; i < LiveCricket.length; i++){%>
          <%if(LiveCricket[i].marketList.match_odd != null){%>
          <%
              var timestamp = `${LiveCricket[i].eventData.time}` * 1000; 
              var date = new Date(timestamp);
              var options = { 
                  year: 'numeric',
                  month: 'long',
                  day: 'numeric',
                  hour: 'numeric',
                  minute: 'numeric',
                  hour12: true
              };
              var formattedTime = date.toLocaleString('en-US', options);
          %>
          <tr class="exchange-pg-mytbl-body-tr" onclick="window.location.href = '/exchange_sports/live_match?id=<%=LiveCricket[i].eventData.eventId%>'; ">
            <!-- <th scope="row"></th> -->
            <td class="name1" id="<%=LiveCricket[i].marketList.match_odd.marketId%>">
                <div class="exchange-pg-mytbl-body-cricket-data-dv">
                    <span class="exchange-pg-mytbl-body-liv-dot"></span>
                    <p>
                        <%=formattedTime%>
                    </p>
                    <p>
                        <b><%=LiveCricket[i].eventData.name%></b>
                    </p>
                </div>
            </td>
            <% let runners = JSON.parse(LiveCricket[i].marketList.match_odd.runners); %>
                  <% let runners1 = []; %>
                  <%let secIds = []; %>
                  <%runners.forEach(item => {%>
                      <%runners1.push(item.runner);%>
                      <%secIds.push(item.secId);%>
                  <%});%>
                  <%for(let j = 0; j < runners1.length; j++){%>
                      <td class="<%=j%>" id="<%=secIds[j]%>"  ></td>
                  <%}%>
          </tr>
          <%cricketNum++%>
      <%}%>
      <%}%>
      <% j = cricketNum %>
      <%for(let i = 0; i < upcomintCricket.length; i++){%>
          <%if(upcomintCricket[i].marketList.match_odd != null){%>
              <%
              var timestamp = `${upcomintCricket[i].eventData.time}` * 1000; 
              var date = new Date(timestamp);
              var options = { 
                  year: 'numeric',
                  month: 'long',
                  day: 'numeric',
                  hour: 'numeric',
                  minute: 'numeric',
                  hour12: true
              };
              var formattedTime = date.toLocaleString('en-US', options);
          %>
              <tr class="exchange-pg-mytbl-body-tr" onclick="window.location.href = '/exchange_sports/live_match?id=<%=upcomintCricket[i].eventData.eventId%>'; ">
                  <!-- <th scope="row">2</th> -->
                  <td class="name1" id="<%=upcomintCricket[i].marketList.match_odd.marketId%>">
                      <div class="exchange-pg-mytbl-body-cricket-data-dv">
                          <%= j%>)
                          <p>
                              <%=formattedTime%>
                          </p>
                          <p>
                              <b><%=upcomintCricket[i].eventData.name%></b>
                          </p>
                      </div>
                  </td>
                  <% let runners = JSON.parse(upcomintCricket[i].marketList.match_odd.runners); %>
                  <% let runners1 = []; %>
                  <%let secIds = []; %>
                  <%runners.forEach(item => {%>
                      <%runners1.push(item.runner);%>
                      <%secIds.push(item.secId);%>
                  <%});%>
                  <%for(let j = 0; j < runners1.length; j++){%>
                      <td class="<%=j%>" id="<%=secIds[j]%>"  ></td>
                  <%}%>
                </tr>
                <%j++%>
          <%}%>
      <%}%>
  </tbody>
</table>
<%}%>




