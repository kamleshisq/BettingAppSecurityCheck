<table class="table table-hover exchange-pg-mytbl">
    <thead class="exchange-pg-mytbl-hed">
      <tr class="exchange-pg-mytbl-hed-tr">
        <th scope="col">Cricket</th>
        <th scope="col">1</th>
        <th scope="col">X</th>
        <th scope="col">2</th>
      </tr>
    </thead>
    <tbody class="exchange-pg-mytbl-body">
        <%for(let i = 0; i < LiveCricket.length; i++){%>
            <%
                var timestamp = `${LiveCricket[i].eventData.time}` * 1000; 
                var date = new Date(timestamp);
                var options = { 
                    year: 'numeric',
                    month: 'long',
                    day: 'numeric',
                    hour: 'numeric',
                    minute: 'numeric',
                    hour12: true
                };
                var formattedTime = date.toLocaleString('en-US', options);
            %>
            <tr class="exchange-pg-mytbl-body-tr">
              <!-- <th scope="row"></th> -->
              <td>
                  <div class="exchange-pg-mytbl-body-cricket-data-dv">
                      <span class="exchange-pg-mytbl-body-liv-dot"></span>
                      <p>
                          <%=formattedTime%>
                      </p>
                      <p>
                          <b><%=LiveCricket[i].eventData.name%></b>
                      </p>
                  </div>
              </td>
              <td>Mark</td>
              <td>Otto</td>
              <td>@mdo</td>
            </tr>
        <%}%>
    </tbody>
  </table>

