                                <% let encodedEventName = encodeURIComponent(result[i].match); %>
                                <% let encodedEventName2 = encodeURIComponent(result[i].event); %>
                                <tr class="acount-stat-tbl-body-tr tbl-data-href" data-href='/gameReport/match?matchName=<%=encodedEventName%>&eventName=<%=encodedEventName2%>'>
