<div class="exchange-pg-inn-banner-col2">
  <div class="exchange-pg-inn-banner-col-titi">
      <div class="exchange-pg-inn-banner-col-titi-txt">
          <h6><span>Bookmaker</span><span class="game-info-popup-spn" data-bs-toggle="modal" data-bs-target="#gameroulspop"> 
            <i class="fa-solid fa-circle-info"></i> 
          </span></h6>
      </div>
      <div class="exchange-pg-inn-banner-col-titi-txt-tim">
          <h6><b>Min : <%=SportLimits.min_stake%>, Max : <%=SportLimits.max_stake%></b></h6>
      </div>
  </div>

  <div class="exchange-pg-inn-tbl">
      <table class="acount-stat-tbl market" id="<%=match.marketList.bookmaker[0].marketId%>">

          <thead class="acount-stat-tbl-hed2">
              <tr class="acount-stat-tbl-hed-tr2" >
                  <th colspan="2" style="text-align: left;" class="bod-red-lft ">Market</th>
                  <th style="text-align: center;" colspan="">Back</th>
                  <th style="text-align: center;" colspan="" class="bod-red-rit">Lay</th>
              </tr>
          </thead>

          <tbody class="acount-stat-tbl-body">
            <%
                    let StringData = match.marketList.bookmaker[0].runners
                    const cleanedString = StringData.replace(/'/g, '');
                    const jsonData = JSON.parse(cleanedString);
                %>
                <%for(let i = 0; i < jsonData.length; i++){%>
                  <tr class="acount-stat-tbl-body-tr tbl-data-href" data-href='#'>
                      <td colspan="2" style=" text-align: left;">
                        <%=jsonData[i].runner%>
                      </td>
                      
                      <td class="tbl-td-with5">
                          <span data-bs-toggle="collapse" href="#bt-slp-colps-match-odds-<%=i%>" role="button" aria-expanded="false" aria-controls="bt-slp-colps-match-odds-<%=i%>" class="tbl-bg-blu-spn bookmaker_blue button" id="<%=jsonData[i].secId%>1">
                              
                              
                          </span> 
                      </td>
                      <td class="tbl-td-with5 ">
                          <span data-bs-toggle="collapse" href="#bt-slp-colps-match-odds-<%=i%>" role="button" aria-expanded="false" aria-controls="bt-slp-colps-match-odds-<%=i%>" class="tbl-bg-pech-spn bookmaker_red button" id="<%=jsonData[i].secId%>2">
                              
                              
                          </span> 
                      </td>
                      
                      
                      
                  </tr>
                  <tr>
                    <td colspan="4">
                      <div class="my-exc-inn-colaps-txt-dv collapse" id="bt-slp-colps-match-odds-<%=i%>" >
                        <%-include("../betslip")%>
                      </div>
                    </td>
                  </tr>
                <%} %>
              
              
          </tbody>

        </table>
        
  </div>

  <div class="exchange-pg-inn-tbl-marqq-dv">
      <div class="exchange-pg-inn-tbl-marqq-dv-wrp">
          <marquee>West Indies Women vs Ireland Women : Advance Toss & Fancy Bets Started in Our Exchange üèè</marquee>
      </div>
  </div>

</div>