<div class="exchange-pg-inn-banner-col2">
  <div class="exchange-pg-inn-banner-col-titi">
      <div class="exchange-pg-inn-banner-col-titi-txt">
          <h6 class="market" id="<%=match.marketList.match_odd.marketId%>"><span>Match Odds</span> &nbsp; <i class="fa-solid fa-circle-info"></i></h6>
      </div>
      <div class="exchange-pg-inn-banner-col-titi-txt-tim">
          <h6><b>Min : <%=SportLimits.min_stake%>, Max : <%=SportLimits.max_stake%></b></h6>
      </div>
  </div>

  <div class="exchange-pg-inn-tbl">
      <table class="acount-stat-tbl">

          <thead class="acount-stat-tbl-hed2">
              <tr class="acount-stat-tbl-hed-tr2" >
                  <th colspan="2" style="text-align: left;" class="bod-red-lft ">Market</th>
                  <th style="text-align: center;" colspan="3">Back</th>
                  <th style="text-align: center;" colspan="3" class="bod-red-rit">Lay</th>
              </tr>
          </thead>
          <% let runners = JSON.parse(match.marketList.match_odd.runners); %>
                <% const runners1 = []; %>
                <%const secIds = []; %>
                <%runners.forEach(item => {%>
                    <%runners1.push(item.runner);%>
                    <%secIds.push(item.secId);%>
                <%});%>
          <tbody class="acount-stat-tbl-body">
            <%for(let i = 0; i < runners1.length; i++){%>
              <tr class="acount-stat-tbl-body-tr tbl-data-href" data-href='#'>
                  <td colspan="2" style=" text-align: left;">
                    <%=runners1[i]%>
                  </td>
                  
                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-blu-spn match_odd_Blue button" id="<%=secIds[i]%>1" >
                                
                    </span>
                  </td>
                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-blu-spn match_odd_Blue button" id="<%=secIds[i]%>2">
                                
                    </span> 
                  </td>

                  

                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-blu-spn match_odd_Blue button" id="<%=secIds[i]%>3">
                                
                    </span>
                  </td>
                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-pech-spn match_odd_Red button" id="<%=secIds[i]%>4">
                                
                    </span>  
                  </td>
                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-pech-spn match_odd_Red button" id="<%=secIds[i]%>5">
                                
                    </span>
                  </td>
                  <td class="tbl-td-with5" >
                    <span class="tbl-bg-pech-spn match_odd_Red button" id="<%=secIds[i]%>6">
                                
                    </span>  
                  </td>
                  
                  
                  
              </tr>
            <%}%>
          </tbody>

        </table>
        <div class="nww-bet-slip-wrp">
          <div class="nww-bet-slip-wrp-col1">
            <div class="nww-bet-slip-wrp-col1-txt">
              <span>Profit <b class="c-gren">2000.00</b></span>
            </div>

            <div class="nww-bet-slip-wrp-col1-txt">
              <span class="nww-bet-slip-wrp-col1-txt-icons">
                <i class="fa-solid fa-plus"></i>
              </span>
              <span class="nww-bet-slip-wrp-col1-txt-num">1.75</span>
              <span class="nww-bet-slip-wrp-col1-txt-icons">
                <i class="fa-solid fa-minus"></i>
              </span>
            </div>

            <div class="nww-bet-slip-wrp-col1-txt">
              <div class="nww-bet-slip-wrp-col1-txt-inn input-group-sm">
                <input type="number" class="form-control set-stake-form-input2" placeholder="Custom" aria-label="Last name">
              </div>
            </div>
          </div>

          <div class="nww-bet-slip-wrp-col2">
            <div class="nww-bet-slip-wrp-col2-inn">
              <span>500</span>
              <span>1000</span>
              <span>2000</span>
              <span>5000</span>
              <span>10000</span>
              <span>20000</span>
              <span>50000</span>
              <span>100000</span>
              <span>200000</span>
            </div>
          </div>

          <div class="nww-bet-slip-wrp-col3">
            <div class="nww-bet-slip-wrp-col3-txt1">
              <p>
                Please select a Stake Amount. Enter a custom amount by clicking on the custom box.
              </p>
            </div>

            <div class="nww-bet-slip-wrp-col3-txt1">
              <a href="#"> Place Bet </a>
            </div>

          </div>

        </div>
  </div>

  <div class="exchange-pg-inn-tbl-marqq-dv">
      <div class="exchange-pg-inn-tbl-marqq-dv-wrp">
          <marquee>West Indies Women vs Ireland Women : Advance Toss & Fancy Bets Started in Our Exchange üèè</marquee>
      </div>
  </div>

</div>

