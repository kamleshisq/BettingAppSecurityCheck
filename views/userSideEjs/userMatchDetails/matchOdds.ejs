  jQuery(document).ready(function(e) {
   jQuery(function() {
      jQuery( "tr.acount-stat-tbl-body-tr" ).insertAfter( jQuery( ".nww-bet-slip-wrp" ) );
      // jQuery( ".content-box .content-side" ).insertBefore( jQuery( ".content-box .left-side" ) ); 
   });
  });
</script>