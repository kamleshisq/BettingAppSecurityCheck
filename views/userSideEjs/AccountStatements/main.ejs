                        <thead class="acount-stat-tbl-hed">
                            <tr class="acount-stat-tbl-hed-tr">
                                <th class="bod-red-lft">SR No.</th>
                                <th>Date & Time</th>
                                <th>Credit</th>
                                <th>Debit</th>
                                <th>Stake</th>
                                <th>Pts</th>
                                <th>Closing</th>
                                <th>Description</th>
                                <th class="bod-red-rit">Remark</th>
                            </tr>
                        </thead>

                        <tbody class="acount-stat-tbl-body">
                            <%if(data.length != 0){%>
                            <%for(let i = 0; i < data.length; i++){%>
                                <%
                                    <!-- var timestamp = `${data[i].time}`;  -->
                                    var date = new Date(data[i].date);
                                    var options = { 
                                        year: 'numeric',
                                        month: 'long',
                                        day: 'numeric',
                                        hour: 'numeric',
                                        minute: 'numeric',
                                        hour12: true
                                    };
                                    var formattedTime = date.toLocaleString('en-US', options);
                                %>
                                <tr class="acount-stat-tbl-body-tr">
                                    <td><%=i+1%></td>
                                    <td><%=formattedTime%></td>
                                    <%if(data[i].creditDebitamount > 0){%>
                                        <td><%=data[i].creditDebitamount%></td>
                                        <td>0</td>
                                    <%}else{%>
                                        <td>0</td>
                                        <td><%=data[i].creditDebitamount%></td>
                                    <%}%>
                                    <%if(data[i].stake){%>
                                        <td><%=data[i].stake%></td>
                                    <%}else{%>
                                        <td>-</td>
                                    <%}%>
                                    <td>0</td>
                                    <td><%=data[i].balance%></td>
                                    <td><%=data[i].description%></td>
                                    <td>-</td>
                                </tr>
                                <%}%>
                                <%}%>
                        </tbody>

                    </table>
                </div>
            </div>

