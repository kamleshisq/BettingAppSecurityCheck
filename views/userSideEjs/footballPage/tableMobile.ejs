                                      <span class="tbl-td-bg-pich-spn mylock-data">
                                        <i class="fa-solid fa-lock"></i>
                                      </span>
                                </div>
                                <div class="exch-mob-data-tbl-card-ro2-dv">
                                  <span class=" 1B" id="<%=secIds[1]%>"> </span>
