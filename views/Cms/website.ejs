<div id="FOOTBALL" class="tabcontent" style="margin-bottom:15px;">
    <div class="dropdown">
      <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false" style="background-color: #fff  !important;color: #000;border: 2px solid #6359CA; padding: 6px 34px;">
        Home Page Manager
      </button>
      <%- include("../dropdownManager")%>
    </div>
    <br>
    <div class="row" style="padding:10px 0px;background-color: #ffff;border-radius: 10px;">
        <div class="container-fluid" id="verticalMenuManager"> 
            <section class="verticalMenuManager" >
                <div class="new-head">
                    <h3 style="font-weight: 700;font-size: 24px;">Vertical Menu Management</h3>
                    <button class="btn-filter" data-bs-toggle="modal" data-bs-target="#myModaladduser"><i class="fa-solid fa-plus"></i> &nbsp; Add Menu</button>
                </div>
                <div class="table_data_details_ col-lg-12">
                  <table>
                    <thead>
                      <tr style="font-size: 11px;">
                        <th>Menu Name</th>
                        <th>Url</th>
                        <th>Number</th>
                        <th>Page Name</th>
                        <th>Edit</th>
                        <th>Delete</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%for(let i = 0; i < verticalMenus.length; i++){%>
                        <%if((i%2)===0){%>
                          <tr class="blue">
                        <%}else{%>
                          <tr>
                        <%}%>
                            <td><%=verticalMenus[i].menuName%></td>
                            <td><%=verticalMenus[i].url%></td>
                            <td><%=verticalMenus[i].num%></td>
                            <td><%=verticalMenus[i].page%></td>
                            <td><button data-bs-toggle="modal" data-bs-target="#myModal5" id="<%=verticalMenus[i]._id%>" class="getVerticalMenuDetails"><i class="fa-solid fa-database"></i></button></td>
                            <td><button class="deleteVerticalMenuDetails" id="<%=verticalMenus[i]._id%>">Delete</button></td>
                          </tr>
                      <%}%>
                    </tbody>
                  </table>
                </div>
            </section>
        </div>
    </div>
    <br><br>
    <div class="row" style="padding:10px 0px;background-color: #ffff;border-radius: 10px;">
      <div class="container-fluid" id="verticalMenuManager"> 
          <section class="verticalMenuManager" >
              <div class="new-head">
                  <h3 style="font-weight: 700;font-size: 24px;">Horizontal Menu Management</h3>
                  <button class="btn-filter" data-bs-toggle="modal" data-bs-target="#myModaladduser1"><i class="fa-solid fa-plus"></i> &nbsp; Add Menu</button>
              </div>
              <div class="table_data_details_ col-lg-12">
                <table>
                  <thead>
                    <th>Menu Name</th>
                    <th>Url</th>
                    <th>Icon</th>
                    <th>Page</th>
                    <th>Number</th>
                    <th>Edit</th>
                    <th>Delete</th>
                  </thead>
                  <%for(let i = 0; i < hosriZontalMenu.length; i++){%>
                    <%if((i%2)===0){%>
                      <tr class="blue">
                    <%}else{%>
                      <tr>
                    <%}%>
                        <td><%=hosriZontalMenu[i].menuName%></td>
                        <td><%=hosriZontalMenu[i].url%></td>
                        <td><img src="../imgForHMenu/<%=hosriZontalMenu[i].icon%>.png" alt="img" class="form__user-photo"></td>
                        <td><%=hosriZontalMenu[i].Number%></td>
                        <td><%=hosriZontalMenu[i].page%></td>
                        <td><button data-bs-toggle="modal" data-bs-target="#myModal6" id="<%=hosriZontalMenu[i]._id%>" class="getVerticalMenuDetails"><i class="fa-solid fa-database"></i></button></td>
                        <td><button class="deleteHorizontalMenuDetails" id="<%=hosriZontalMenu[i]._id%>">Delete</button></td>
                      </tr>
                  <%}%>
                </table>
              </div>
          </section>
      </div>
  </div>
</div>

