<div id="FOOTBALL" class="tabcontent" style="margin-bottom:15px;">
    <div class="dropdown">
      <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false" style="background-color: #fff  !important;color: #000;border: 2px solid #6359CA; padding: 6px 34px;">
        Home Page Manager
      </button>
      <%- include("../dropdownManager")%>
    </div>
    <br>
    <div class="row" style="padding:10px 0px;background-color: #ffff;border-radius: 10px;">
        <div class="container-fluid" id="verticalMenuManager"> 
            <section class="verticalMenuManager" >
                <div class="new-head">
                    <h3 style="font-weight: 700;font-size: 24px;">Vertical Menu Management</h3>
                    <button class="btn-filter" data-bs-toggle="modal" data-bs-target="#myModaladduser"><i class="fa-solid fa-plus"></i> &nbsp; Add Menu</button>
                </div>
                <div class="table_data_details_ col-lg-12">
                  <table>
                    <thead>
                      <tr style="font-size: 11px;">
                        <th>Menu Name</th>
                        <th>Url</th>
                        <th>Number</th>
                        <th>Page Name</th>
                        <th>Edit</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%for(let i = 0; i < verticalMenus.length; i++){%>
                        <%if((i%2)===0){%>
                          <tr class="blue">
                        <%}else{%>
                          <tr>
                        <%}%>
                            <td><%=verticalMenus[i].menuName%></td>
                            <td><%=verticalMenus[i].url%></td>
                            <td><%=verticalMenus[i].num%></td>
                            <td><%=verticalMenus[i].page%></td>
                            <td><button data-bs-toggle="modal" data-bs-target="#myModal5" id="<%=verticalMenus[i].page%>" class="getVerticalMenuDetails"><i class="fa-solid fa-database"></i></button></td>
                          </tr>
                      <%}%>
                    </tbody>
                  </table>
                </div>
            </section>
        </div>
    </div>
</div>

<div class="modal fade" id="myModaladduser" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
        </div>
        <div class="modal-header" style="text-align: center;">
          <h4 class="modal-title" style="color: #fff;">Add Menu</h4>
        </div>
        <div class="modal-body">
          <form class="form-data" id="Add-User">
            <div class="row">
              <div class="col">
                <label for="menuName">Name</label>
                <input type="text" class="form-control ml0" placeholder="" name="menuName">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="url">url</label>
                <input type="text" class="form-control ml0" placeholder="Abitest" name="url">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="num">Number</label>
                <input type="number" class="form-control ml0" placeholder="" name="num">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="page">Page</label>
                <input type="text" class="form-control ml0" placeholder="0" name="page">
              </div>
            </div>
            <button type="submit" class="btn-submit mt-3" style="background-color: #46BCAA;">Add</button>
          </form>
        </div>
      </div>
    </div>
  </div>



  <div class="modal fade" id="myModal5" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
        </div>
        <div class="modal-header" style="text-align: center;">
          <h4 class="modal-title" style="color: #fff;">User Details</h4>
        </div>
        <div class="modal-body">
          <form class="form-data" id="edit-form">
            <div class="row">
              <div class="col">
                <label for="">Name</label>
                <input type="text" class="form-control ml0" name="name">
                <input type="hidden" id="id" name="id">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">User Name</label>
                <input type="text" class="form-control ml0" name="userName">
              </div>
              <div class="col">
                <label for="">User Role</label>
              </div>
            </div>
            
            <div class="row mt-2">
              <div class="col">
                <label for="">Share <span style="color: #46BCAA;">100</span></label>
                <input type="text" class="form-control ml0" placeholder="90" name="share">
              </div>
              <div class="col">
                <label for="">My Share</label>
                <input type="text" class="form-control ml0" placeholder="10" name="myShare">
              </div>
              <div class="col">
                <label for="">Actual</label>
                <input type="text" class="form-control ml0" placeholder="10" name="actual">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Credit Reference</label>
                <input type="text" class="form-control ml0" name="cdReference">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="exposureLimit">Expose Limits</label>
                <input type="number" class="form-control ml0" placeholder="0" name="exposureLimit">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Note</label>
                <textarea id="w3review" placeholder="" name="w3review" rows="3" cols="50" style="width: 100%;border-radius: 5px;"></textarea>
              </div>
            </div>
          
  
            <button type="submit" class="btn-submit mt-3">Save</button>
          </form>
        </div>
      </div>
    </div>
  </div>