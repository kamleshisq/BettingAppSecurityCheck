<div id="FOOTBALL" class="tabcontent" style="margin-bottom:15px;">
    <div class="dropdown">
      <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false" style="background-color: #fff  !important;color: #000;border: 2px solid #6359CA; padding: 6px 34px;">
        Home Page Manager
      </button>
      <%- include("../dropdownManager")%>
    </div>
    <br>
    <div class="row" style="padding:10px 0px;background-color: #ffff;border-radius: 10px;">
        <div class="container-fluid" id="verticalMenuManager"> 
            <section class="verticalMenuManager" >
                <div class="new-head">
                    <h3 style="font-weight: 700;font-size: 24px;">Vertical Menu Management</h3>
                    <button class="btn-filter" data-bs-toggle="modal" data-bs-target="#myModaladduser"><i class="fa-solid fa-plus"></i> &nbsp; Add Menu</button>
                </div>
            </section>
        </div>
    </div>
</div>

<div class="modal fade" id="myModaladduser" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
        </div>
        <div class="modal-header" style="text-align: center;">
          <h4 class="modal-title" style="color: #fff;">Add Account</h4>
        </div>
        <div class="modal-body">
          <form class="form-data" id="Add-User">
            <div class="row">
              <div class="col">
                <label for="">Name</label>
                <input type="text" class="form-control ml0" placeholder="" name="name">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">User ID</label>
                <input type="text" class="form-control ml0" placeholder="Abitest" name="userName">
              </div>
              <div class="col">
                <label for="">User Role</label>
                
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Password</label>
                <input type="password" class="form-control ml0" placeholder="" name="password">
              </div>
              <div class="col">
                <label for="">Confirm Password</label>
                <input type="password" class="form-control ml0" placeholder="" name="passwordConfirm">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Share</label>
                <input type="text" class="form-control ml0" placeholder="0" name="email">
              </div>
              <div class="col">
                <label for="">Visible</label>
                <input type="text" class="form-control ml0" placeholder="0" name="pswd">
              </div>
              <div class="col">
                <label for="">Actual</label>
                <input type="text" class="form-control ml0" placeholder="0" name="pswd">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Credit Reference</label>
                <input type="text" class="form-control ml0" placeholder="0" name="email">
              </div>
              <div class="col">
                <label for="exposureLimit">Expose Limits</label>
                <input type="number" class="form-control ml0" placeholder="0" name="exposureLimit">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Note</label>
                <input type="text" class="form-control ml0" placeholder="" name="email">
              </div>
            </div>
            <div class="row mt-2">
              <div class="col">
                <label for="">Password</label>
                <input type="password" class="form-control ml0" placeholder="" name="email">
                <i class="bi bi-eye-slash" id="togglePassword"></i>
              </div>
            </div>
            <button type="submit" class="btn-submit mt-3" style="background-color: #46BCAA;">Add</button>
          </form>
        </div>
      </div>
    </div>
  </div>