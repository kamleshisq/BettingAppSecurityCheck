  <script>
    const customOptionCheckbox = document.getElementById('customOptionCheckbox');
    const newWhiteLabelInput = document.getElementById('newWhiteLabelInput');
    const SElectWhiteLable = document.getElementById("whiteLabel")
  
    customOptionCheckbox.addEventListener('change', function () {
      if (this.checked) {
        newWhiteLabelInput.classList.remove('hide')
        // Hide the select element
        SElectWhiteLable.classList.add('hide')
      } else {
        newWhiteLabelInput.classList.add('hide')
        SElectWhiteLable.classList.remove('hide')
      }
    });
  </script>
  <script>
  
    document.getElementById('roleSelect').addEventListener('change', function () {
      // console.log("WORK")
      if (document.getElementById('roleSelect').value === '6492fe4fd09db28e00761694') {  
        document.getElementById('shareVisibleActual').classList.add("hide") 
      } else {
        document.getElementById('shareVisibleActual').classList.remove("hide") 
      }

      if (document.getElementById('roleSelect').value === '6492fe4fd09db28e00761694') {  
        document.getElementById('ExposureLimits').classList.remove("hide") 
      } else {
        document.getElementById('ExposureLimits').classList.add("hide") 
      }

      if (document.getElementById('roleSelect').value === '650bccdbb3fdc8c922c34bbe') {  
        document.getElementById('operator-authorization').classList.remove("hide") 
      } else {
        document.getElementById('operator-authorization').classList.add("hide") 
      }
    });
  </script>
  <script>
    $(document).on("change", ".bookmakerType", function(e){
      e.preventDefault()
      // console.log($(this).val())
      let html=``
      if($(this).val() == "NET_LOSS"){
        html += ` <option value="0.25">0.25</option>
                      <option value="0.5">0.5</option>
                      <option value="0.75">0.75</option>
                      <option value="1">1</option>
                      <option value="1.25">1.25</option>
                      <option value="1.5">1.5</option>
                      <option value="1.75">1.75</option>
                      <option value="2">2</option>
                      <option value="2.25">2.25</option>
                      <option value="2.50">2.50</option>
                      <option value="2.75">2.75</option>
                      <option value="3">3</option>`
      }else if ($(this).val() == "ENTRY"){
        html += `<option value="0.025">0.025</option>
                      <option value="0.05">0.05</option>
                      <option value="0.075">0.075</option>
                      <option value="0.1">0.1</option>
                      <option value="0.125">0.125</option>
                      <option value="0.15">0.15</option>
                      <option value="0.175">0.175</option>
                      <option value="0.2">0.2</option>
                      `
      }else if ($(this).val() == "ENTRY_LOSS_"){
        html += `<option value="0.25">0.25</option>
                      <option value="0.5">0.5</option>
                      <option value="0.75">0.75</option>
                      <option value="1">1</option>`
      }
      document.getElementById("bookmakerPer").innerHTML = html
    })
  </script>

    
