<section>
    <div class="container-fluid">
        <div class="table-data-section">
            
            <div class="data-table">
              <div class="rowId" data-rowid="1"></div>
  <table class="bet-list-table">
      <%if(series.length > 0){%>
        <thead>
            <tr>
                <th>Competition</th>
                <th>Min Stake</th>
                <th>Max Stake </th>
                <th>Max Profit</th>
                <th>Max Odd</th>
                <th>Delay</th>
                <th>Actions </th>
                </tr>
        </thead>
        <tbody class="new-body">
  
  
  
  
            <!-- CRICKET -->
            <%for(let i = 0; i < series.length; i++){%>
              <%
                
                let limitData = betLimit.find(item => item.type == series[i].series)
              %>
              <%if(limitData){%>
                <tr id="<%=i + 1%>">
                  <td id="<%=series[i].series%>" class="btn-filter"><a href="/admin/betlimit/sports/event?event=<%=series[i].series%>"><%=series[i].series%></a></td>
                  <td><input type="text" class="form-datas" name="min_stake" value="<%=limitData.min_stake%>" ></td>
                  <td><input type="text" class="form-datas" name="max_stake" value="<%=limitData.max_stake%>" ></td>
                  <td><input type="text" class="form-datas" name="max_profit" value="<%=limitData.max_profit%>" ></td>
                  <td><input type="text" class="form-datas" name="max_odd" value="<%=limitData.max_odd%>" ></td>
                  <td><input type="text" class="form-datas" name="delay" value="<%=limitData.delay%>" ></td>
                  <td>
                    <div class="btn-group">
                      <button type="button" class="btn update updateBetLimit">Update</button>
                    </div>
                  </td>
                </tr>
              <%}else{%>
                <tr id="<%=i + 1%>">
                    <td id="<%=series[i].series%>" class="btn-filter"><a href="/admin/betlimit/sports/event?event=<%=series[i].series%>"><%=series[i].series%></a></td>
                    <td><input type="text" class="form-datas" name="min_stake" value="0" ></td>
                    <td><input type="text" class="form-datas" name="max_stake" value="0" ></td>
                    <td><input type="text" class="form-datas" name="max_profit" value="0" ></td>
                    <td><input type="text" class="form-datas" name="max_odd" value="0" ></td>
                    <td><input type="text" class="form-datas" name="delay" value="0" ></td>
                    <td>
                      <div class="btn-group">
                        <button type="button" class="btn update updateBetLimit">Update</button>
                      </div>
                    </td>
                  </tr>
              <%}%>
            <%}%>
        </tbody>
    <%}else{%>
        <tr class="empty_table"><td>No <%=gameName%> Series! </td></tr>
    <%}%>
  </table>
            </div>
        </div>
    </div>
  </section>