<table>
    <thead>
      <tr>
        <th>User Name</th>
        <th>Won</th>
        <th>Lost</th>
        <th>Profit/Lost</th>
      </tr>
    </thead>
    <tbody class="new-body">
      <%if(users.length !== 0){%>
      <%for(let i = 0; i < users.length; i++){%>
      <%if((i%2) == 0){%>
      <tr>
        <%}else{%>
        <tr>
          <%}%>
          <td><%=users[i].userName%></td>
          <td><%=users[i].Won%></td>
          <td><%=users[i].Loss%></td>
          <td><%=users[i].myPL%></td>
        </tr>
        <%}%>
      </tr>
      <%}else{%>
        <tr class="empty_table"><td>No record found</td></tr>
      <%}%>
    </tbody>
  </table>