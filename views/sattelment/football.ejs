<div class="data-table">
    <table>
      <thead>
          <tr>
            <th>S.No</th>
            <th>Event Date</th>
            <th>Series</th>
            <th>Event Name</th>
            <th>Total Bets</th>
            <th>Open Bets</th>
            <th></th>
          </tr>
      </thead>
      <tbody class="new-body" id="football-tbody">
        <%if(betsEventWise.length !== 0){%>
        <% let betsEventWiseFootball = betsEventWise.find(item => item.id == 'Football') %>
        <%if(betsEventWiseFootball){%>
        <%console.log(betsEventWiseFootball, '===> betsEventWiseFootball')%>
        <% let betsEventWiseData2Football = betsEventWiseFootball.data%>
        <%for(let i = 0; i < betsEventWiseData2Football.length; i++){ %>
          
          <tr>
            <%
                var timestamp = betsEventWiseData2Football[i].eventdate; 
                var date = new Date(timestamp);
                var options = { 
                  year: 'numeric',
                  month: 'long',
                  day: 'numeric',
                  hour: 'numeric',
                  minute: 'numeric',
                  hour12: true
              };
              var formattedTime = date.toLocaleString('en-US', options);
               
            %>
            <td><%=i+1 %></td>
            <td class="date-time"><%=formattedTime%></td>
            <td class="text-nowrap"><%= betsEventWiseData2Football[i].series%></td>
            <td class="text-nowrap"><%= betsEventWiseData2Football[i].matchName%></td>
            <td><%= betsEventWiseData2Football[i].count%></td>
            <td><%= betsEventWiseData2Football[i].count2%></td>
            <td><a href="/admin/settlementIn?id=<%=betsEventWiseData2Football[i].eventid%>" class="btn-green">settle</a></td>
          </tr>
        <%} %>
        <%}else{%>
          <tr class="empty_table"><td>No record found</td></tr>
        <%}%>
        <%}else{%>
          <tr class="empty_table"><td>No record found</td></tr>
        <%}%>
      </tbody>
    </table>
  </div>