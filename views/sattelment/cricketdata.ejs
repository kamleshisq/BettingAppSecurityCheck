<div class="data-table">
    <table>
      <thead>
          <tr>
            <th>S.No</th>
            <th>Event Date</th>
            <th>Series</th>
            <th>Event Name</th>
            <th>Total Bets</th>
            <th>Open Bets</th>
            <th></th>
          </tr>
      </thead>
      <tbody class="new-body">
        <%if(betsEventWise.length !== 0){%>
        <%for(let i = 0; i < betsEventWise.length; i++){ %>
          
          <tr>
            <%
                var timestamp = betsEventWise[i].eventdate * 1000; 
                var date = new Date(timestamp);
                var options = { 
                  year: 'numeric',
                  month: 'long',
                  day: 'numeric',
                  hour: 'numeric',
                  minute: 'numeric',
                  hour12: true
              };
              var formattedTime = date.toLocaleString('en-US', options);
               
            %>
            <td><%=i+1 %></td>
            <td class="date-time"><%=formattedTime%></td>
            <td class="text-nowrap"><%= betsEventWise[i].series%></td>
            <td class="text-nowrap"><%= betsEventWise[i].matchName%></td>
            <td><%= betsEventWise[i].count%></td>
            <td><%= betsEventWise[i].count%></td>
            <td><a href="/admin/settlementIn?id=<%=betsEventWise[i].eventid%>" class="btn-green">settle</a></td>
          </tr>
        <%} %>
        <%}else{%>
          <tr class="empty_table"><td>No record found</td></tr>
        <%}%>
      </tbody>
    </table>
  </div>