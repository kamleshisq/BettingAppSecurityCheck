<%- include("../header") %>
  <!--Container Main start-->
  <div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head bet-monitoring-heading"> 
          <h3>Withdrawal request</h3>
          <form action="" class="betmonitor-heading-from">
            <div class="input-group">
                <label class="up-down-form">Select User</label>
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-magnifying-glass"></i></span>
              </div>
              <input type="text" id="searchUser" list="text_editors" class="form-control searchUser" placeholder="search user" aria-label="Username" aria-describedby="basic-addon1" autocomplete="off">
              <div class="wrapper">
                <ul id="search" class="users">
              
                </ul>
                <ul id="button" class="button">
                  
                </ul>
              </div>
              
            </div>
            <div class="input-group">
              <label class="up-down-form">From Date</label>
              <input type="date" id="fromDate">
            </div>
            <div class="input-group">
              <label class="up-down-form">To Date</label>
              <input type="date" id="toDate">
            </div>
            <div class="input-group">
              <label>Status</label>
              <select name="result" id="status">
                <option value="All" selected>All</option>
                <option value="pending">Approval Pending</option>
                <option value="approved">Approved</option>
                <option value="denied">Denie</option>
              </select>
            </div>
            
            <div class="input-group">
              <button type="button" class="refresh btn-group">Refresh</button>
            </div>
          </form>
         
      </div>
  </section>

  <section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading">
                <h3>Alert Bet</h3>
                <div class="table-heading-btn">
                  
                </div>
            </div>
            <div class="data-table">
                <%- include("./table") %>
            </div>
        </div>
    </div>
</section>
<div class="pageId" data-pageid="1"></div>
</div>
<div id="load-more" style="display: none;">
  
  <button class="load-more">Load More</button>
  
</div>


<div class="modal fade" id="APPROVE" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header">
        <h4 class="modal-title">Approve</h4>
      </div>
      <div class="modal-body">
        <form class="form-data Approv_form">
          <div class="row">
            <div class="col">
              <label for="">Edit Amount</label>
              <input type="number" class="form-control ml0" placeholder="" name="approvedamount">
            </div>
          </div>
          <p>Once you approve the application, it can not be undone later. Would you like to proceed?</p>
          <div class="row">
            <div class="col">
              <label for="customOptionCheckbox">
              <input type="checkbox" id="customOptionCheckbox" name="status">
              I understand. Proceed to approve the application.
              </label>
            </div>
          </div>
          <button type="submit" class="btn-submit mt-3">OK</button>
        </form>
      </div>
    </div>
    
  </div>
</div>


<div class="modal fade" id="DENY" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title">Deny</h4>
      </div>
      <div class="modal-body">
        <form class="form-data denyWithorowel_form">
          <div class="row">
            <div class="col">
              <label for="">Remark</label>
              <input type="text" class="form-control ml0" placeholder="" name="remark" required>
            </div>
          </div>
          <div class="row">
            <div class="col">
              <label for="customOptionCheckbox">
              <input type="checkbox" id="customOptionCheckbox" name="status">
              I understand. Proceed to cancle the application.
              </label>
            </div>
          </div>
          <button type="submit" class="btn-submit mt-3">OK</button>
        </form>
      </div>
    </div>
    
  </div>
</div>



<div class="modal fade payment-method-data-popup" id="myModal4" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-bs-dismiss="modal">Ã—</button>
        </div>
        <div class="modal-header">
          <!-- <button type="button" class="close" data-dismiss="modal"></button> -->
          <h4 class="modal-title">Account Details</h4>
        </div>
        <div class="modal-body" id="AccountData">
        
        </div>
      </div>
    </div>
  </div>
  
    

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/apexcharts@3.40.0/dist/apexcharts.min.js"></script>
<script type="text/javascript" src="../js/bundle.js"></script>
<script>
    $(document).on('click', '.btn-default', function(e){
        $(this).closest('.modal-content').find('.payment-tab-content').html('')
    })
</script>

</body>
</html>