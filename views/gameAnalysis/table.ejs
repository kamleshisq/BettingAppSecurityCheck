<table>
    <thead>
        <tr >
          <th>S.No</th>
          <th>White Label</th>
          <th>Active User</th>
          <th>Total Bets</th>
          <th>Won</th>
          <th>Lost</th>
          <th>Void </th>
          <th>Open </th>
          <th>P/L</th>
        </tr>
    </thead>
    <tbody class="new-body">
        <%for(let i = 0; i<gameAnalist.length; i++){%>
          <%if(i % 2 == 0){%>
            <tr class="blue">
          <%}else{%>
            <tr  >
          <%}%>
            <td><%=i + 1%></td>
            <td><%=gameAnalist[i]._id%></td>
            <td><%=activeUser[i].activeUser%></td>
            <td><%=gameAnalist[i].betcount%></td>
            <td> <%=gameAnalist[i].won%> </td>
            <td><%=gameAnalist[i].loss%></td>
            <td> - </td>
            <td> <%=gameAnalist[i].open%> </td>
            <%if(gameAnalist.returns > 0){%>
              <td style="color:#46BCAA">+<%=gameAnalist[i].returns%></td>
            <%}else{%>
              <td style="color:#FE3030"><%=gameAnalist[i].returns%></td>
            <%}%>
          </tr>
        <%}%>
        
       
      </tbody>
  </table>