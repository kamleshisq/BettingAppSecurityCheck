<div class="container-fluid">
  <div class="meDetails" id="meDatails" data-me='<%=JSON.stringify(me)%>'></div>
  <div class="currentUserDetails" id="currentUserDetails" data-currentuser='<%=JSON.stringify(currentUser)%>'></div>
  <div class="pageLink" data-page="1"></div>
  <span data-rowid="1" class="rowId"></span>
  <div class="table-data-section">
    <div class="table-heading">
      <h3><svg width="21" height="20" viewBox="0 0 21 20" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path d="M2 0.5H16C16.5304 0.5 17.0391 0.710714 17.4142 1.08579C17.7893 1.46086 18 1.96957 18 2.5V9.58C16.4954 9.32723 14.951 9.65613 13.68 10.5H10V14.5H11.08C10.97 15.18 10.97 15.85 11.08 16.5H2C1.46957 16.5 0.960859 16.2893 0.585786 15.9142C0.210714 15.5391 0 15.0304 0 14.5V2.5C0 1.4 0.9 0.5 2 0.5ZM2 4.5V8.5H8V4.5H2ZM10 4.5V8.5H16V4.5H10ZM2 10.5V14.5H8V10.5H2ZM21 19.5V18.5C21 17.17 18.33 16.5 17 16.5C15.67 16.5 13 17.17 13 18.5V19.5H21ZM17 11.5C15.9 11.5 15 12.4 15 13.5C15 14.6 15.9 15.5 17 15.5C18.1 15.5 19 14.61 19 13.5C19 12.39 18.11 11.5 17 11.5Z" fill="#6359CA"/>
        </svg>
         &nbsp;User Management</h3>
      <div class="table-heading-btn">
        <button class="btn-filter" id="downloadBtn"><i class="fa-solid fa-download"></i> &nbsp; Excel</button>
        <%if(currentUser.role.authorization.includes('createDeleteUser')){%>
          <button class="btn-filter" data-bs-toggle="modal" data-bs-target="#myModaladduser"><i
              class="fa-solid fa-plus"></i> &nbsp; Add User</button>
          <%}%>
      </div>
    </div>
    <div class="data-table">
      <table id="table12">
        <thead>
          <tr>
            <th>S.No</th>
            <th>User Name</th>
            <th>White Labels</th>
            <th>Credit Reference</th>
            <th>Balance</th>
            <th>Available Balance</th>
            <th>Downlevel Balance</th>
            <th>Client P/L</th>
            <th>Upline P/L</th>
            <th>Exposure Limit</th>
            <th>Exposure</th>

            <!-- <th>Exposure<br>0.00</th> -->
            <th>Lifetime Credit</th>
            <th>Lifetime Deposit</th>
            <th colspan="6">Action</th>




          </tr>
        </thead>
        <tbody class="new-body">
          <%for(let i=0; i<users.length; i++){%>
            <%if((i+1) % 2 !=0){%>
              <tr class="blue trtable" id="<%=i+1%>" data-id="<%=users[i]._id%>">
                <%}else{%>
              <tr class="trtable" id="<%=i+1%>" data-id="<%=users[i]._id%>">
                <%}%>
                  <td>
                    <%=i + 1%>
                  </td>
                  <td class="getOwnChild">
                    <%if(users[i].roleName !='user' ){%>
                      <a href="/admin/userManagement?id=<%=users[i]._id%>">
                        <%=users[i].userName%>
                      </a>

                      <%}else{%>
                        <%=users[i].userName%>
                          <%}%>
                  </td>
                  <td>
                    <%=users[i].whiteLabel%>
                  </td>
                  <td>
                    <%=users[i].creditReference%>
                  </td>
                  <td>
                    <%=users[i].balance%>
                  </td>
                  <td>
                    <%=users[i].availableBalance%>
                  </td>
                  <td>
                    <%=users[i].downlineBalance%>
                  </td>
                  <td style="color:#FE3030;">
                    <%=users[i].clientPL%>
                  </td>
                  <td>
                    <%=users[i].uplinePL%>
                  </td>
                  <td>
                    <%=users[i].exposureLimit%>
                  </td>
                  <td>
                    <%=users[i].exposure%>
                  </td>
                  <!-- <td><%=users[i].userName%></td> -->
                  <td>
                    <%=users[i].lifeTimeCredit%>
                  </td>
                  <td>
                    <%=users[i].lifeTimeDeposit%>
                  </td>
                  <td>
                    <%if(currentUser.role.authorization.includes('accountControl')){%>

                      <button data-bs-toggle="modal" data-bs-target="#myModal" title="Deposit/Withdrawal" class="Deposite"> D/W </button>
                      <%}%>
                  </td>
                  <%if(users[i].roleName==="Super-Duper-Admin" ) {%>
                    <td><button data-bs-toggle="modal" data-bs-target="#myModal2" title="commission"  class="CreaditChange">C</button></td>
                    <%} %>
                      <td>
                        <%if(currentUser.role.authorization.includes('changeUserPassword')){%>

                          <button data-bs-toggle="modal" data-bs-target="#myModal3" title="Change Password" class="PasswordChange">P</button>
                          <%}%>
                      </td>
                      <td>
                        <%if(currentUser.role.authorization.includes('betLockAndUnloack')){%>

                          <button type="button" class="betLockStatus" title="Betlock">B</button>
                          <%}%>
                      </td>
                      <td class="text-purple">
                        <%if(currentUser.role.authorization.includes('userStatus')){%>

                          <button data-bs-toggle="modal" data-bs-target="#myModal4" title="Change Status" class="StatusChange">CS</button>
                          <%}%>
                      </td>
                      <td>
                        <%if(currentUser.role.authorization.includes('userName')){%>

                          <button class="UserDetails" title="View Details"><i class="fa-solid fa-database"></i></button>
                          <%}%>
                      </td>
              </tr>
              <%}%>

        </tbody>
      </table>
    </div>
  </div>
</div>
</div>