<%- include("../header") %>
<!--Container Main start-->
<div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head"> 
        <h3>Catalog Management</h3>
      </div>
    </div>
  </section>

  <section>
    <div class="container-fluid">
      <div class="table-data-section">
        <div class="data-table">
          <table>
            <thead>
              <tr>
                <th>S.No</th>
                <th>Enabled</th>
                <th>Competition Name</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody class="new-body">
              <% for(let i = 0; i < data.length; i++){%>
              <%if((i%2)==0){%>
              <tr>
                <%}else{%>
                <tr>
                  <%}%>
                  <td><%=i + 1%></td>
                  <td>
                    <%if(data[i].status){%>
                    <input type="checkbox" class="status_check" data-id="<%=data[i].compId%>"  checked>
                    <%}else{%>
                    <input type="checkbox" class="status_check" data-id="<%=data[i].compId%>">
                    <%}%>
                  </td>
                  <td><%=data[i].name%></td>
                  <td>
                    <div class="btn-group">
                      <button class="series_list"> <a href="/admin/catalogcontrol/compitations/events?compId=<%=data[i].compId%>&sportId=<%=data[i].sportId%>">Event List</a></button>
                    </div>
                  </td>
                  <%}%>
                </tr>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </section>

  <div class="pageId" data-pageid="1"></div>

</div>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/apexcharts@3.40.0/dist/apexcharts.min.js"></script>

</body>
</html> 