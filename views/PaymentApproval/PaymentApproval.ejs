<%- include("../header") %>
  <!--Container Main start-->
  <div class="dashboard-main-wraper">
  <section class="dashboard-welcome-section">
    <div class="container-fluid">
      <div class="new-head bet-monitoring-heading"> 
          <h3>Approvals</h3>
          <form action="" class="betmonitor-heading-from">
            <div class="input-group">
              <label class="up-down-form">Select User</label>
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-magnifying-glass"></i></span>
              </div>
              <input type="text" id="searchUser" list="text_editors" class="form-control searchUser" placeholder="search user" aria-label="Username" aria-describedby="basic-addon1" autocomplete="off">
              <div class="wrapper">
                <ul id="search" class="users">
              
                </ul>
                <ul id="button" class="button">
                  
                </ul>
              </div>
              
            </div>
            <div class="input-group">
              <label class="up-down-form">From Date</label>
              <input type="date" id="fromDate">
            </div>
            <div class="input-group">
              <label class="up-down-form">To Date</label>
              <input type="date" id="toDate">
            </div>
            <div class="input-group">
              <label>Status</label>
              <select name="result" id="status">
                <option value="All" selected>All</option>
                <option value="pending">Approval Pending</option>
                <option value="approved">Approved</option>
                <option value="denied">Denie</option>
              </select>
            </div>
            <div class="input-group">
              <label class="up-down-form">Type</label>
              <select name="Sport" id="type">
                <option value="All" selected>All</option>
                <option value="banktransfer">Bank Transfer</option>
                <option value="upi">UPI</option>
                <option value="paytm">Paytm</option>
              </select>
            </div>
            
            <div class="input-group">
              <button type="button" class="refresh btn-group">Refresh</button>
            </div>
          </form>
         
      </div>
  </section>

  <section>
    <div class="container-fluid">
        <div class="table-data-section">
            <div class="table-heading">
                <h3>Alert Bet</h3>
                <div class="table-heading-btn">
                  
                </div>
            </div>
            <div class="data-table">
              <%- include("./table") %>
            </div>
        </div>
    </div>
</section>
<div class="pageId" data-pageid="1"></div>
</div>


<div class="modal fade" id="myModaladduser" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">&times;</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
        <h4 class="modal-title">Approve</h4>
      </div>
      <div class="modal-body">
        <form class="form-data paymentreq_form">
          <div class="row">
            <div class="col">
              <label for="">Edit Amount</label>
              <input type="number" class="form-control ml0" placeholder="" name="approvedamount">
              <input type="hidden" name="id">
            </div>
          </div>
          <p>Once you approve the application, it can not be undone later. Would you like to proceed?</p>
          <div class="row">
            <div class="col">
              <label for="customOptionCheckbox">
              <input type="checkbox" id="customOptionCheckbox" name="status">
              I understand. Proceed to approve the application.
              </label>
            </div>
          </div>
          <button type="submit" class="btn-submit mt-3">OK</button>
        </form>
      </div>
    </div>
    
  </div>
</div>

<div class="modal fade " id="myModal2" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">×</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal"></button> -->
        <h4 class="modal-title">Voide Bets</h4>
      </div>
      <div class="modal-body">
        <form class="form-data timely-voideBet">
          <div class="row">
              <div class="col">
                <label for="">password</label>
                <input type="password" class="form-control ml0" name="password">
              </div>
          </div>
          <div class="row">
            <div class="col">
              <label >Remark</label>
              <input type="text" class="form-control ml0"  name="Remark">
            </div>
          </div>
        <button type="submit" class="btn-submit mt-3 ">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>

<div class="modal fade " id="myModal3" role="dialog">
  <div class="modal-dialog">
  
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-bs-dismiss="modal">×</button>
      </div>
      <div class="modal-header">
        <!-- <button type="button" class="close" data-dismiss="modal"></button> -->
        <h4 class="modal-title">Image</h4>
      </div>
      <div class="modal-body">
      <img src="" alt="">
      </div>
    </div>
  </div>
</div>
  
    

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/apexcharts@3.40.0/dist/apexcharts.min.js"></script>
