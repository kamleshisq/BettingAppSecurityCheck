<table>
  <thead>
    <tr>
      <th>S.No</th>
      <th>User Name</th>
      <th>Date</th>
      <th>Time</th>
      <th>Sport</th>
      <th>Match</th>
      <th>Market</th>
      <th>Bet On</th>
      <th>Odds</th>
      <th>Status</th>
      <th>Stake</th>
      <th>Returns</th>
      <th>Txn ID</th>
      <th>Game/Round</th>
    </tr>
  </thead>
  <tbody class="new-body">
    <% for(let i = 0; i < bets.length; i++){%>
    <% let date = new Date(bets[i].date)%>
    <%if((i%2)==0){%>
    <tr>
      <%}else{%>
      <tr>
        <%}%>
        <td><%=i + 1%></td>
        <td><%=bets[i].userName%></td>
        <td class="date-time"><%=date.getDate() + '-' +(date.getMonth() + 1) + '-' + date.getFullYear()%></td>
        <td class="date-time"><%=date.getHours() + ':' + date.getMinutes() +':' + date.getSeconds()%></td>
        <%if(bets[i].match){%>
        <td>-</td>
        <td class="text-nowrap"><%=bets[i].match%></td>
        <td><%=bets[i].marketName%></td>
        <td><%=bets[i].selectionName%></td>
        <td><%=bets[i].oddValue%></td>
        <%}else{%>
        <td>-</td>
        <td>-</td>
        <td>-</td>
        <td>-</td>
        <td>-</td>
        <%}%>
        <td><%=bets[i].status%></td>
        <td><%=bets[i].Stake%></td>
        <td><%=bets[i].returns%></td>
        <td><%=bets[i].transactionId%></td>
        <td><%=bets[i].event%></td>
      </tr>
      <%}%>
    </tr>
  </tbody>
</table>